<view class="tabbar"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['tabbar-item',(selectedIndex===item.index)?'active':'']}}" data-index="{{item.index}}" data-event-opts="{{[['tap',[['onTabClick',['$event']]]]]}}" bindtap="__e"><image class="tabbar-item-icon" src="{{selectedIndex===item.index?item.selectedIconPath:item.iconPath}}"></image><text class="{{[(true)?'tabbar-text':'',(selectedIndex===item.index)?'active':'']}}">{{''+item.text+''}}</text></view></block></view>